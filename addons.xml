    <disclaimer lang="en_gb" />
    <language />
    <platform>all</platform>
    <license />
    <forum />
    <website />
    <email />
    <source />
  </extension>
</addon><addon id="repository.johtru.com" name="jojo Repository" provider-name="jojo" version="1.0.0">
