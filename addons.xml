		<checksum>https://raw.githubusercontent.com/trummerjo/repository.johtru.com/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/trummerjo/repository.johtru.com/master/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Kodi Repository from jojo.</summary>
